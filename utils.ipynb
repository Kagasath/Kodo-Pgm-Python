    "# utils.py\n",
    "\n",
    "def is_prime(n):\n",
    "    \"\"\"Return True if n is a prime number, else False.\"\"\"\n",
    "    if n <= 1:\n",
    "        return False\n",
    "    for i in range(2,n+1):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "    return True"
   ]
